    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "typedoc": "^0.25.11"
  },
  "dependencies": {
    "chalk": "^5.3.0",
